
{{log model}}
<div class="jumbo">
  <h2>{{model.nazivOglasa}}</h2>
   
    <div class="detail owner">
      <span>Owner:</span> {{model.korisnik.ime}} 
    </div>
    <div class="detail type">
      <span>Type:</span> {{model.vrstaOglasa}}
    </div>
    <div class="detail location">
      <span>Squares:</span> {{model.kvadratura}}
    </div>
    <div class="detail bedrooms">
      <span>Number of bedrooms:</span> {{model.brojProstorija}}
    </div>
    <div class="detail location">
      <span>Location:</span> {{model.lokacija.adresa}}
    </div>
    <div class="detail location">
      <span>Price:</span> {{model.cijena}} BAM
    </div>
   </div>